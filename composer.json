{
    "name": "lara-zeus/delia",
    "description": "delia is a bookmark manager for your filament app",
    "keywords": [
        "laravel",
        "lara-zeus",
        "delia",
        "bookmark",
        "manager",
        "favorites",
        "filamentphp"
    ],
    "homepage": "https://larazeus.com/delia",
    "support": {
        "issues": "https://github.com/lara-zeus/delia/issues",
        "source": "https://github.com/lara-zeus/delia"
    },
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "php coder",
            "email": "info@larazeus.com",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "^8.1",
        "lara-zeus/core": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest-plugin-livewire": "2.x-dev",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "laravel/pint": "^1.0",
        "larastan/larastan": "^2.2",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "LaraZeus\\Delia\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraZeus\\Delia\\Tests\\": "tests"
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint",
        "test:pest": "vendor/bin/pest --parallel",
        "test:phpstan": "vendor/bin/phpstan analyse",
        "test": [
            "@test:pest",
            "@test:phpstan"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraZeus\\Delia\\DeliaServiceProvider"
            ],
            "aliases": {
                "Delia": "LaraZeus\\Delia\\Facades\\Delia"
            }
        }
    },
    "prefer-stable": true,
    "minimum-stability": "stable"
}
